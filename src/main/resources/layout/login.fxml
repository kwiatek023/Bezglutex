<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="layout.controllers.LoginController">
    <children>
        <TextField fx:id="login" layoutX="400.0" layoutY="300.0" prefHeight="40.0" prefWidth="250.0"
                   promptText="Login"/>
        <PasswordField fx:id="password" layoutX="400.0" layoutY="350.0" prefHeight="40.0"
                       prefWidth="250.0" promptText="Password"/>
        <!--suppress JavaFxUnresolvedFxIdReference -->
        <Button fx:id="loginButton" layoutX="400.0" layoutY="400.0" mnemonicParsing="false"
                prefHeight="30.0" prefWidth="250.0" onAction="#logIn" text="Log in"/>
    </children>
</AnchorPane>